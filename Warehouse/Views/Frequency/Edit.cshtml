@model Warehouse.Models.Frequency
@{
    ViewBag.Title = "Edit Frequency";
}

<div class="col-md-12">
    <section class="panel">
        <header class="panel-heading">
            Edit Frequency
        </header>
        <div class="panel-body">
            <form method="put">
                <div class="form-group">
                    <label asp-for="Value"></label>
                    @Html.TextBoxFor(f => f.Value, new { @class = "form-control" })
                </div>
                <div class="form-group">
                    <a role="button" class="btn btn-default" asp-controller="Frequency" asp-action="Index">Cancel</a>
                    <input class="btn btn-info" type="submit" value="Update" />
                </div>
            </form>
            <form method="delete">
                <div class="form-group">
                    <input class="btn btn-danger" type="submit" value="Delete" />
                </div>
            </form>
        </div>
    </section>
</div>
